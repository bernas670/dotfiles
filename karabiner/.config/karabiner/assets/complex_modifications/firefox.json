{
  "title": "Firefox Linux Keybinds",
  "rules": [
    {
      "description": "Firefox developer tools",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [ "^org\\.mozilla\\.firefox$" ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "i",
            "modifiers": { "mandatory": [ "control", "shift" ] }
          },
          "to": [
            {
              "key_code": "i",
              "modifiers": [
                "left_command", "left_option"
              ]
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "bundle_identifiers": [ "^org\\.mozilla\\.firefox$" ],
              "type": "frontmost_application_if"
            }
          ],
